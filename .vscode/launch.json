	"version": "0.2.0",
	"configurations": [
		{
			"preLaunchTask": "build",
			"name": "(lldb) Adjuntar",
			"type": "cppdbg",
			"request": "attach",
			"program": "${workspaceFolder}/checker",
			"processId": "${command:pickProcess}",
			"MIMode": "lldb"
		},
		{
			"preLaunchTask": "build",
			"name": "(lldb) Iniciar",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/checker",
			"args": ["5", "2", "1", "3", "9", "0"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb"
		}
	]
}
